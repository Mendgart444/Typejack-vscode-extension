{
  "name": "typejack-support",
  "displayName": "Typejack Support",
  "version": "0.0.1",
  "main": "./client/out/extension.js",
  "scripts": {
    "compile:server": "tsc -p server/tsconfig.json",
    "compile:client": "tsc -p client/tsconfig.json",
    "compile": "pnpm run compile:server && pnpm run compile:client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mendgart444/typejack-vscode-extension"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:toml",
    "onFileSystem:file"
  ],
  "contributes": {
    "languages": [
      {
        "id": "toml",
        "aliases": [
          "Toml",
          "toml"
        ],
        "extensions": [
          ".toml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "toml",
        "scopeName": "source.toml.typejack",
        "path": "./syntaxes/typejack.tmLanguage.json"
      }
    ]
  }
}
